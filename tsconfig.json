{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strict": true,
    "isolatedModules": true,
    "verbatimModuleSyntax": true,
    "baseUrl": ".",
    "types": [
      "astro/client",
      "@cloudflare/workers-types"
    ],
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      }
    ],
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  },
  "exclude": [
    "node_modules"
  ]
}
